<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
                                  http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0" metadata-complete="false">
    <!-- Managed beans, navigation rules, etc. go here -->



    <!-- This file must be present with our jar, even if it is empty.  
         Otherwise, our Java annotations won't get scanned! -->

    <managed-bean>
        <managed-bean-name>login</managed-bean-name>
        <managed-bean-class>com.java.jsf.Login</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>loginDao</managed-bean-name>
        <managed-bean-class>com.java.jsf.LoginDao</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>employ</managed-bean-name>
        <managed-bean-class>com.java.jsf.Employ</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>employDao</managed-bean-name>
        <managed-bean-class>com.java.jsf.EmployDao</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>demo</managed-bean-name>
        <managed-bean-class>com.java.jsf.Demo</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>namebean</managed-bean-name>
        <managed-bean-class>com.java.jsf.NameBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>calculation</managed-bean-name>
        <managed-bean-class>com.java.jsf.Calculation</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

</faces-config>
